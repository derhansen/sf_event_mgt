{
  "name": "derhansen/sf_event_mgt_build",
  "description": "composer.json for builds",
  "repositories": [
    {
      "type": "path",
      "url": "../"
    }
  ],
  "require": {
    "derhansen/sf_event_mgt": "@dev",
    "typo3/cms-core": "^13.2 || dev-main",
    "php": ">=8.2"
  },
  "require-dev": {
    "typo3/cms-composer-installers": "^5.0",
    "typo3/cms-backend": "^13.2 || dev-main",
    "typo3/cms-frontend": "^13.2 || dev-main",
    "typo3/cms-extbase": "^13.2 || dev-main",
    "typo3/cms-felogin": "^13.2 || dev-main",
    "typo3/cms-fluid-styled-content": "^13.2 || dev-main",
    "typo3/cms-fluid": "^13.2 || dev-main",
    "typo3/cms-tstemplate": "^13.2 || dev-main",
    "typo3/cms-install": "^13.2 || dev-main",
    "typo3/cms-extensionmanager": "^13.2 || dev-main",
    "typo3/testing-framework": "dev-main",
    "friendsofphp/php-cs-fixer": "^3.12.0",
    "saschaegerer/phpstan-typo3": "^1.8.3",
    "phpstan/extension-installer": "^1.1",
    "jangregor/phpstan-prophecy": "^1.0",
    "codeception/codeception": "^5.0",
    "codeception/module-webdriver": "^4.0",
    "koehnlein/codeception-email-mailpit": "^0.3.0",
    "codeception/module-db": "^3.1"
  },
  "config": {
    "allow-plugins": {
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true,
      "phpstan/extension-installer": true
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DERHANSEN\\SfEventMgt\\Tests\\": "../Tests"
    }
  }
}
